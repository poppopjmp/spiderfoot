{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Active Scans",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "sf_active_scans", "legendFormat": "Active" }],
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 3 }, { "color": "red", "value": 5 }] } } }
    },
    {
      "title": "Total Scans Completed",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "sf_scans_total{status=\"completed\"}", "legendFormat": "Completed" }],
      "fieldConfig": { "defaults": { "color": { "mode": "fixed", "fixedColor": "blue" } } }
    },
    {
      "title": "Events Processed (24h)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "increase(sf_events_total[24h])", "legendFormat": "Events" }],
      "fieldConfig": { "defaults": { "color": { "mode": "fixed", "fixedColor": "purple" } } }
    },
    {
      "title": "High-Risk Findings (24h)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "increase(sf_findings_total{risk_level=~\"high|critical\"}[24h])", "legendFormat": "High/Critical" }],
      "fieldConfig": { "defaults": { "color": { "mode": "fixed", "fixedColor": "red" } } }
    },
    {
      "title": "API Latency (p95)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "histogram_quantile(0.95, rate(sf_request_duration_seconds_bucket[5m]))", "legendFormat": "p95" }],
      "fieldConfig": { "defaults": { "unit": "s", "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] } } }
    },
    {
      "title": "LLM Token Usage",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "increase(sf_llm_tokens_total[24h])", "legendFormat": "Tokens (24h)" }],
      "fieldConfig": { "defaults": { "color": { "mode": "fixed", "fixedColor": "orange" } } }
    },
    {
      "title": "Event Rate by Category",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "rate(sf_events_total[5m])", "legendFormat": "{{ event_category }}" }],
      "fieldConfig": { "defaults": { "custom": { "fillOpacity": 20, "lineWidth": 2 } } }
    },
    {
      "title": "Risk Level Distribution",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "sf_findings_total", "legendFormat": "{{ risk_level }}" }]
    },
    {
      "title": "Module Execution Time (Top 10)",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "expr": "topk(10, sf_module_duration_seconds_sum / sf_module_duration_seconds_count)", "legendFormat": "{{ module }}" }],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "title": "Service Logs",
      "type": "logs",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 12 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [{ "expr": "{service=\"spiderfoot\"}", "legendFormat": "" }],
      "options": { "showTime": true, "sortOrder": "Descending", "enableLogDetails": true }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [{ "expr": "rate({service=\"spiderfoot\", level=\"ERROR\"} | json [5m])", "legendFormat": "Errors/s" }],
      "fieldConfig": { "defaults": { "custom": { "fillOpacity": 30 }, "color": { "mode": "fixed", "fixedColor": "red" } } }
    },
    {
      "title": "Enrichment Pipeline Activity",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "rate(sf_enrichment_files_processed_total[5m])", "legendFormat": "Files processed/s" },
        { "expr": "rate(sf_enrichment_findings_total[5m])", "legendFormat": "Findings/s" },
        { "expr": "sf_enrichment_queue_depth", "legendFormat": "Queue depth" }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["spiderfoot", "overview"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "SpiderFoot â€” Overview",
  "uid": "sf-overview",
  "version": 1
}
