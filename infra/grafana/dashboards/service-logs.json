{
  "annotations": { "list": [] },
  "description": "Container log exploration and analysis via Loki — powered by Vector.dev pipeline",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Log Pipeline Health",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "green" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["sum"] }
      },
      "title": "Total Log Lines (24h)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"spiderfoot\"} [24h]))",
          "legendFormat": "Lines"
        }
      ]
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 3,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["sum"] }
      },
      "title": "Error Lines (1h)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"spiderfoot\", level=~\"ERROR|CRITICAL\"} [1h]))",
          "legendFormat": "Errors"
        }
      ]
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "yellow" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["sum"] }
      },
      "title": "Warning Lines (1h)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"spiderfoot\", level=\"WARNING\"} [1h]))",
          "legendFormat": "Warnings"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 5,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "title": "Vector Events/s",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(spiderfoot_component_sent_events_total[5m]))",
          "legendFormat": "Events/s"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "purple" },
          "unit": "bytes"
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "title": "Vector Bytes/s",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(spiderfoot_component_sent_bytes_total[5m]))",
          "legendFormat": "Bytes/s"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 7,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "title": "Vector Errors",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(spiderfoot_component_errors_total[5m]))",
          "legendFormat": "Errors/s"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 10,
      "title": "Log Volume & Distribution",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": { "mode": "normal" }
          },
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "ERROR" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }]
          },
          {
            "matcher": { "id": "byName", "options": "WARNING" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }]
          },
          {
            "matcher": { "id": "byName", "options": "INFO" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
          },
          {
            "matcher": { "id": "byName", "options": "DEBUG" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 11,
      "options": { "tooltip": { "mode": "multi" } },
      "title": "Log Volume by Level",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (level) (count_over_time({service=\"spiderfoot\"} | json [1m]))",
          "legendFormat": "{{ level }}"
        }
      ]
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 60,
            "stacking": { "mode": "normal" }
          },
          "color": { "mode": "palette-classic" }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 12,
      "options": { "tooltip": { "mode": "multi" } },
      "title": "Log Volume by Service",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (job) (count_over_time({service=\"spiderfoot\"} [1m]))",
          "legendFormat": "{{ job }}"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 20,
      "title": "Error Log Stream",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 15 },
      "id": 21,
      "title": "Errors & Warnings",
      "type": "logs",
      "targets": [
        {
          "expr": "{service=\"spiderfoot\", level=~\"ERROR|CRITICAL|WARNING\"}",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "wrapLogMessage": true
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 30,
      "title": "All Logs Explorer",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 24 },
      "id": 31,
      "title": "All Service Logs",
      "type": "logs",
      "targets": [
        {
          "expr": "{service=\"spiderfoot\"}",
          "legendFormat": ""
        }
      ],
      "options": {
        "showTime": true,
        "sortOrder": "Descending",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "wrapLogMessage": true,
        "showCommonLabels": true
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 },
      "id": 40,
      "title": "Vector Pipeline Metrics",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 37 },
      "id": 41,
      "options": { "tooltip": { "mode": "multi" } },
      "title": "Vector Sink Throughput",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(spiderfoot_component_sent_events_total{component_kind=\"sink\"}[5m])",
          "legendFormat": "{{ component_id }}"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 37 },
      "id": 42,
      "options": { "tooltip": { "mode": "multi" } },
      "title": "Vector Source Ingestion",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(spiderfoot_component_received_events_total{component_kind=\"source\"}[5m])",
          "legendFormat": "{{ component_id }}"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["spiderfoot", "logs", "vector", "loki"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "SpiderFoot — Service Logs",
  "uid": "sf-service-logs",
  "version": 1
}
