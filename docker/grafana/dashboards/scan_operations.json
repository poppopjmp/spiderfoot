{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "SpiderFoot Scan Operations Dashboard",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Active Scans",
      "type": "stat",
      "targets": [
        {
          "expr": "sf_scans_active",
          "legendFormat": "Active"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Queued Scans",
      "type": "stat",
      "targets": [
        {
          "expr": "sf_scans_queued",
          "legendFormat": "Queued"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "green", "mode": "fixed" }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Total Scans Started",
      "type": "stat",
      "targets": [
        {
          "expr": "sf_scans_started_total",
          "legendFormat": "Started"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "green", "mode": "fixed" }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Completed Scans",
      "type": "stat",
      "targets": [
        {
          "expr": "sf_scans_completed_total",
          "legendFormat": "Completed"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "red", "mode": "fixed" }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Failed Scans",
      "type": "stat",
      "targets": [
        {
          "expr": "sf_scans_failed_total",
          "legendFormat": "Failed"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "purple", "mode": "fixed" }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Total Events",
      "type": "stat",
      "targets": [
        {
          "expr": "sf_events_produced_total",
          "legendFormat": "Events"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "title": "Scan Throughput",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 20, "stacking": { "mode": "none" } }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "title": "Scans Started (rate)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(sf_scans_started_total[5m])",
          "legendFormat": "Started / 5m"
        },
        {
          "expr": "rate(sf_scans_completed_total[5m])",
          "legendFormat": "Completed / 5m"
        },
        {
          "expr": "rate(sf_scans_failed_total[5m])",
          "legendFormat": "Failed / 5m"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 30 }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "title": "Events Produced (rate)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(sf_events_produced_total[5m])",
          "legendFormat": "Events / 5m"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 102,
      "title": "Scan Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "title": "Scan Duration Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sf_scan_duration_seconds{quantile=\"0.5\"}",
          "legendFormat": "p50"
        },
        {
          "expr": "sf_scan_duration_seconds{quantile=\"0.9\"}",
          "legendFormat": "p90"
        },
        {
          "expr": "sf_scan_duration_seconds{quantile=\"0.99\"}",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "bars", "fillOpacity": 80 }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "title": "Scan Status Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sf_scan_status_count",
          "legendFormat": "{{ status }}"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "id": 103,
      "title": "Event Breakdown",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "id": 30,
      "title": "Top Event Types",
      "type": "piechart",
      "targets": [
        {
          "expr": "topk(10, sf_event_type_total)",
          "legendFormat": "{{ type }}"
        }
      ],
      "options": {
        "legend": { "placement": "right", "displayMode": "table" },
        "pieType": "donut"
      }
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "id": 31,
      "title": "Top Modules by Events",
      "type": "piechart",
      "targets": [
        {
          "expr": "topk(10, sf_module_events_total)",
          "legendFormat": "{{ module }}"
        }
      ],
      "options": {
        "legend": { "placement": "right", "displayMode": "table" },
        "pieType": "donut"
      }
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 104,
      "title": "Module Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 33 },
      "id": 40,
      "title": "Module Execution Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(sf_modules_executed_total[5m])",
          "legendFormat": "Modules / 5m"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["spiderfoot", "osint", "scans"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "SpiderFoot - Scan Operations",
  "uid": "spiderfoot-scan-ops",
  "version": 1
}
